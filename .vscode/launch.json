{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run FastAPI (AI4OHS-HYBRID)",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "src.ohs.api.main:app",
        "--reload",
        "--host",
        "127.0.0.1",
        "--port",
        "8000"
      ],
      "cwd": "${workspaceFolder}",
      "envFile": "${workspaceFolder}/.env",
      "justMyCode": true,
      "console": "integratedTerminal"
    },
    {
      "name": "Run Pipeline 00â†’03",
